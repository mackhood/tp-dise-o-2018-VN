{{#partial "contenido"}}
    <h1>Dispositivos que se pueden dar de alta</h1>

    <table class="table table-stripped">
        <tr>
            <th>Nombre</th>
            <th>Equipo Concreto</th>
            <th>Consumo estimado por hora</th>
            <th>Id</th>

        </tr>

        {{#each dispositivos}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{equipoConcreto}}</td>
                <td>{{consumoEstimadoPorHora}}</td>
                <td>{{id}}</td>
                <td><a href="/usuario/optimizar/{{id}}">Alta</a><td>

            </tr>
        {{/each}}
    </table>
{{/partial}}

{{>home/usuario.hbs}}