{{#partial "contenido"}}

<style>
	
  table.one {
  position:relative;
  float:left;
  border:solid 1px green;
  width:45%;
}

  table.two {
  position:relative;
  float:right;
  border:solid 1px red;
  width:45%;
}

</style>

    <table class="one">
        <tr>
            <th> Inicio </th>
            <th> Fin </th>
        </tr>
    
    {{#each consumos}}
        <tr>
        	<td>{{inicio}}</td>
        	<td>{{fin}}</td>
       	</tr> 
    {{/each}}
    
    </table>
	
	<table class="two">
        <tr>
            <th> Consumo</th>
        </tr>
    
    {{#each valores}}
        <tr>
        	<td>{{this}}</td>
       	</tr> 
    {{/each}}
    
    </table>
	
	<br> <br>
	{{#block "sinconsumos"}} {{/block}}
	<br> <br> <br> <br>
	<div align="center">
  	<a class="btn btn-info btn-cancel" role="button" href="/usuario/dispositivo"> Regresar </a>
	</div>
{{/partial}}

{{>home/usuario.hbs}}